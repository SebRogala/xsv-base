{
  "name": "sebrogala/xsv-base",
  "description": "Module with some utils on which other modules can base in Zend Expressive",
  "type": "library",
  "homepage": "https://github.com/SebRogala/xsv-base",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Sebastian Rogala",
      "homepage": "http://sebrogala.com/"
    }
  ],
  "require": {
    "zendframework/zend-config-aggregator": "^1.0",
    "zendframework/zend-inputfilter": "^2.8"
  },
  "require-dev": {
    "composer/composer": ">=1.0.0-alpha10",
    "phpunit/phpunit": "~5.0"
  },
  "autoload": {
    "psr-4": {
      "Xsv\\Base\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "XsvTest\\Base\\": "test/"
    }
  }
}
